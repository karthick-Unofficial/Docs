            "description": "",
            "date": true
        },
        "message": {
            "description": "",
            "type": "string"
        },
        "userId": {
            "description": "",
            "type": "string"
        },
        "viewed": {
            "description": "",
            "type": "boolean"
        },
        "escalationEvent": {
            "description": "",
            "type": "string"
        },
        "audioSettings": {
            "description": "",
            "type": "object"
        },
        "dismissId": {
            "description": "",
            "type": "string"
        },
        "history": {
            "description": "",
            "type": "array",
            "items": {
                "type": "object",
